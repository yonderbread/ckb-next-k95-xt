<plist version="1.0">
<dict>
    <key>Label</key>
    <string>org.ckb-next</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Applications/ckb-next.app/Contents/MacOS/ckb-next</string>
        <string>--background</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/tmp/ckb-next.log</string>
    <key>StandardOutPath</key>
    <string>/tmp/ckb-next.log</string>
</dict>
</plist>
